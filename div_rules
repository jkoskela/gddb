%project_0(P) :- dividend(P,_).
project_0(P) :- dividend(P,X).
product_1(P,D) :- project_0(P), divisor(D).
except_2(P,D) :- product_1(P,D), not dividend(P,D).
%project_3(P) :- except_2(P,_).
project_3(P) :- except_2(P,X).
quotient(Q) :- project_0(Q), not project_3(Q).

